#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin>>t;
	while(t--)
	{
	    int x,y,z;
	    cin>>x>>y>>z;
	    int sum=x-1+y-1+z-1;
	   // if(sum==0)
	   // {
	   //     cout<<"no"<<endl;
	   // }
	   if(sum<=2)
	   {
	       cout<<"no"<<endl;
	   }
	   // else if(sum%2==0)
	   // {
	   //     cout<<"no"<<endl;
	   // }
	    else
	    {
	        cout<<"yes"<<endl;
	    }
	    
	}
	
	return 0;
}
