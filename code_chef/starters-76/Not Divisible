#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    int n;
	    cin>>n;
	    for(int j=0;j<n;j++)
	    {
	        if(j%2==0)
	        {
	            cout<<1<<" ";
	        }
	        else
	        {
	            cout<<0<<" ";
	        }
	    }
	    cout<<endl;
	}
	
	return 0;
}
